<%= form_for @dog do |f| %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.label :color %>
  <%= f.text_field :color %>

  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>

  <%= f.label :friendly %>
  <%= f.text_field :friendly %>

  <%= f.label :history %>
  <%= f.text_field :history %>

  <%= f.submit 'Add a breed' %>
<% end %>